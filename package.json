{
	"name": "@downwindcss/variant-group",
	"version": "1.0.0",
	"author": "dance2die",
	"module": "dist/variant-group.esm.js",
	"license": "MIT",
	"main": "dist/index.js",
	"typings": "dist/index.d.ts",
	"files": [
		"dist",
		"src"
	],
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"start": "tsdx watch",
		"build": "tsdx build",
		"test": "tsdx test",
		"lint": "tsdx lint",
		"prepare": "tsdx build",
		"size": "size-limit",
		"analyze": "size-limit --why",
		"version:alpha": "pnpm version prerelease --preid=alpha",
		"publish:alpha": "pnpm publish --tag alpha",
		"version:beta": "pnpm version prerelease --preid=beta",
		"publish:beta": "pnpm publish --tag beta"
	},
	"publishConfig": {
		"access": "public"
	},
	"husky": {
		"hooks": {
			"pre-commit": "tsdx lint"
		}
	},
	"prettier": {
		"printWidth": 80,
		"semi": true,
		"singleQuote": true,
		"trailingComma": "es5"
	},
	"size-limit": [
		{
			"path": "dist/variant-group.cjs.production.min.js",
			"limit": "1.5 KB"
		},
		{
			"path": "dist/variant-group.esm.js",
			"limit": "1.5 KB"
		}
	],
	"devDependencies": {
		"@size-limit/preset-small-lib": "^4.12.0",
		"@types/lodash.zipwith": "^4.2.6",
		"husky": "^6.0.0",
		"size-limit": "^4.12.0",
		"tsdx": "^0.14.1",
		"tslib": "^2.3.0",
		"typescript": "^4.3.4"
	},
	"dependencies": {
		"lodash.zipwith": "^4.2.0",
		"xregexp": "^5.0.2"
	}
}
